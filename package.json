{

    "name": "sample",
    "license": "UNLICENSED",
    "jest": {
        "verbose": true,
        "collectCoverageFrom": [
            "**/*.{js,jsx}",
            "!**/node_modules/**",
            "!**/vendor/**",
            "!**/coverage/**"
        ],
        "coverageThreshold": {
            "global": {
                "branches": 80,
                "functions": 80,
                "lines": 80,
                "statements": -10
            }
        }
    },
    "scripts": {
        "test": "jest --coverage"
    },
    "dependencies": {
        "lodash": "^4.17.20"
    },
    "devDependencies": {
        "jest": "^26.4.0"
    }
}